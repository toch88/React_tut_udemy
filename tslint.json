{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-unused-variable": true,
    "curly": true,
    "class-name": true,
    "semicolon": true,
    "triple-equals": true,
    "no-console":false
  },
  "defaultSeverity": "warning",
  "linterOptions": {    
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }  
}
